<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="pollinatorconservation.views.FXMLFloweringTableController">
   <children>
      <Label layoutX="55.0" layoutY="32.0" text="Seleccione una planta florifera y dé clic en el botón &quot;Consultar&quot;." />
      <TableView fx:id="floweringTableView" layoutX="68.0" layoutY="84.0" prefHeight="232.0" prefWidth="464.0">
        <columns>
          <TableColumn fx:id="genericNameTableColumn" prefWidth="147.0" resizable="false" text="Nombre genérico" />
          <TableColumn fx:id="scientificNameTableColumn" prefWidth="165.0" resizable="false" text="Nombre científico" />
            <TableColumn fx:id="cladeTableColumn" prefWidth="151.0" resizable="false" text="Clado" />
        </columns>
      </TableView>
      <Button fx:id="checkButton" disable="true" layoutX="32.0" layoutY="343.0" mnemonicParsing="false" onAction="#checkButtonClick" prefHeight="31.0" prefWidth="89.0" text="Consultar" />
      <Button layoutX="498.0" layoutY="353.0" mnemonicParsing="false" onAction="#cancelButtonClick" prefHeight="25.0" prefWidth="80.0" text="Cancelar" />
   </children>
</AnchorPane>
